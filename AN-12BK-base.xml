	      <adf n="0">
		<mode>adf</mode>
		<serviceable type="bool">false</serviceable>
		<frequencies>
		  <selected-khz type="int">490</selected-khz>  <!-- NDB "DVI" (Donaueschingen) -->
		  <standby-khz type="int">401</standby-khz>    <!-- EDTM ancient military airbase -->
