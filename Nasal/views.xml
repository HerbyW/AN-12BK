<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -45 </chase-distance-m>

  <view n="100">
    <name>Co Pilote</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.55884 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.59504 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -13.25107 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg> 77 </default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Engineer</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  -0.20828 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.59504 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -12.53191 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg> 77 </default-field-of-view-deg>
      <heading-offset-deg type="double"> 90 </heading-offset-deg>
    </config>
  </view>
  
  <view n="102">
    <name>Tail View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.00000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  0.62727 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 15.02763 </z-offset-m> <!--Back-->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 50 </default-field-of-view-deg>
      <heading-offset-deg type="double"> 180 </heading-offset-deg>
    </config>
  </view>

  <view n="103">
    <name>Exterior tail View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.00000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  5.41105 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 13.45989 </z-offset-m> <!--Back-->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 50 </default-field-of-view-deg>
    </config>
  </view>
  
  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">  -0.55884 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.59504 </y-offset-m> <!-- Up    -->
