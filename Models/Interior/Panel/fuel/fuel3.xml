<PropertyList>
 <path>fuel1.ac</path>


 
 <animation>
  <type>rotate</type>
  <object-name>n1</object-name>
  <property>/IL-76/instrumentation/fuel/E3/n1</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep> 0.0 </dep></entry> 

   <entry><ind> 25000</ind><dep>  270</dep></entry>
   <entry><ind> 30000</ind><dep>  274</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>n2</object-name>
  <property>/IL-76/instrumentation/fuel/E3/n2</property>
  <interpolation>
    <entry><ind>  0.0</ind><dep> 0.0 </dep></entry> 

   <entry><ind> 7000</ind><dep>  270</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
               <!--Lights-->
                                   
 <animation>
   <type>material</type>
   <object-name>fuel_m</object-name>
   <object-name>n1</object-name>
   <object-name>n2</object-name>
   <emission>
	<red-prop>/IL-76/instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>


 </PropertyList>

