<PropertyList>
 <path>fuel_sum.ac</path>


 
 <animation>
  <type>rotate</type>
  <object-name>n1</object-name>
  <property>/IL-76/instrumentation/fuel/sum/n1</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep> 0.0 </dep></entry> 

   <entry><ind> 100000</ind><dep>  300</dep></entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 

 
 
 
               <!--Lights-->
                                   
 <animation>
   <type>material</type>
   <object-name>fuel_s</object-name>
   <object-name>n1</object-name>
    <emission>
	<red-prop>/IL-76/instrumentation/lights/centr_panel/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/centr_panel/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/centr_panel/int-blue</blue-prop>
	</emission>
</animation>


 </PropertyList>

