<PropertyList>
 <path>flaps_ind.ac</path>


<animation>
  <type>rotate</type>
  <object-name>block1</object-name>
  <property>/IL-76/instrumentation/Flaps_ind/f1</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>    -68</dep></entry> 
   <entry><ind> 0.33</ind><dep>  -45</dep></entry>
   <entry><ind> 0.66</ind><dep>  -22</dep></entry>
   <entry><ind> 1</ind><dep>  0</dep></entry>
   <entry><ind> 1</ind><dep>  0</dep></entry>
  </interpolation>
  <center>
   <x-m>0.000</x-m>
   <y-m>0.041</y-m>
   <z-m>0.001</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>block2</object-name>
  <property>/IL-76/instrumentation/Flaps_ind/f2</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>    -68</dep></entry> 
   <entry><ind> 0.33</ind><dep>  -45</dep></entry>
   <entry><ind> 0.66</ind><dep>  -22</dep></entry>
   
   <entry><ind> 1</ind><dep>  0</dep></entry>
  </interpolation>
  <center>
   <x-m>0.000</x-m>
   <y-m>0.041</y-m>
   <z-m>0.001</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>block3</object-name>
  <property>/IL-76/instrumentation/Flaps_ind/p1</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>    -68</dep></entry> 
  
   <entry><ind> 14</ind><dep>  -30</dep></entry>
  
   <entry><ind> 25</ind><dep>  0</dep></entry>
  </interpolation>
  <center>
   <x-m>0.000</x-m>
   <y-m>0.041</y-m>
   <z-m>0.001</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>block4</object-name>
  <property>/IL-76/instrumentation/Flaps_ind/p2</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>    -68</dep></entry> 
  
   <entry><ind> 14</ind><dep>  -30</dep></entry>
  
   <entry><ind> 25</ind><dep>  0</dep></entry>
  </interpolation>
  <center>
   <x-m>0.000</x-m>
   <y-m>0.041</y-m>
   <z-m>0.001</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

              <!--Lights-->
                                   
 <animation>
   <type>material</type>
   <object-name>flaps_ind</object-name>
   <emission>
		<red>0.8</red>
		<green>0.0</green>
		<blue>0.0</blue>
		<factor-prop>/IL-76/instrumentation/lights/panelR/int-red</factor-prop>
	</emission>
</animation>

<animation>
   <type>material</type>
   <object-name>flaps2</object-name>
   <emission>
		<red>0.4</red>
		<green>0.0</green>
		<blue>0.0</blue>
		<factor-prop>/IL-76/instrumentation/lights/panelR/int-red</factor-prop>
	</emission>
</animation>

 <animation>
   <type>material</type>
   <object-name>block1</object-name>
   <object-name>block2</object-name>
   <object-name>block3</object-name>
   <object-name>block4</object-name>
   <emission>
	<red-prop>/IL-76/instrumentation/lights/panelR/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/panelR/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/panelR/int-blue</blue-prop>
	</emission>
</animation>

 </PropertyList>

