<PropertyList>
 <path>skawk.ac</path>

 

    <animation>	
	<object-name>s1</object-name>
	<object-name>r1</object-name>
	<object-name>r2</object-name>
	<object-name>r3</object-name>
	<object-name>r4</object-name>
        <object-name>r5</object-name>
	<object-name>group</object-name>
     </animation>

  <!-- working for Rembrandt -->

  <animation>
    <type>material</type>
        <object-name>s1</object-name>
	<object-name>r1</object-name>
	<object-name>r2</object-name>
	<object-name>r3</object-name>
	<object-name>r4</object-name>
        <object-name>r5</object-name>
	<object-name>group</object-name>
    <emission>
      <factor-prop>tu154/switches/gauge-light</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

 
 <!--MTSCH Schwitch left --> 

 <animation>
  <type>rotate</type>
  <object-name>s1</object-name>
  <property>systems/kurs_mp/pu_2/s1</property>
  <factor>1.0</factor>

  <interpolation>
    <entry><ind>1</ind>	<dep>0</dep></entry>    
    <entry><ind>2</ind>	<dep>30</dep></entry>       
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
</animation>


<animation>
  <type>pick</type> 
  <object-name>s1</object-name> 
  <action>
  <button>3</button> 
  <repeatable>true/</repeatable> 
  <interval-sec>0.2</interval-sec>
  <binding>
  <command>property-adjust</command> 
  <property>systems/kurs_mp/pu_2/s1</property>
  <step>1</step>
  <min>1</min>
  <max>2</max> 
   </binding>
  </action>
</animation>
  
  
<animation>
  <type>pick</type> 
  <object-name>s1</object-name> 
  <action>
  <button>4</button> 
  <repeatable>true/</repeatable> 
  <interval-sec>0.2</interval-sec>
  <binding>
  <command>property-adjust</command> 
  <property>systems/kurs_mp/pu_2/s1</property>
  <step>-1</step>
  <min>1</min>
  <max>2</max> 
   </binding>
  </action>
</animation>
  

