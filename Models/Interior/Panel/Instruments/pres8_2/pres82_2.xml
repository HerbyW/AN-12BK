<PropertyList>
 <path>pres8_2.ac</path>

   <!--Lights-->
                                   
 <animation>   
   <object-name>group</object-name>
   
</animation>

  <animation>
    <type>material</type>
    <object-name>group</object-name>
   
    <emission>
      <factor-prop>tu154/switches/gauge-light</factor-prop>
      <red>   0.35 </red>
      <green> 0.35 </green>
      <blue>  0.35 </blue>
    </emission>
  </animation>

 
 
 
 <animation>
  <type>rotate</type>
  <object-name>group</object-name>
  <property>/systems/apu/oil_t</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>0</dep></entry> 
   <entry><ind> 400</ind><dep>  90</dep></entry>
   <entry><ind> 680</ind><dep>  180</dep></entry>
   <entry><ind> 900</ind><dep>  235</dep></entry>
  
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 </PropertyList>
