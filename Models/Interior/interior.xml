<!-- Doors animations -->
<animation>
<name>IntLaPorteduCote</name>
<object-name>intporte</object-name>
<object-name>intportetour</object-name>
<object-name>intvitreporte</object-name>
</animation>

<animation>
<type>pick</type>
<object-name>IntLaPorteduCote</object-name>
<visible>true</visible>
<action>
<button>0</button>
<repeatable>false</repeatable>
<binding>
<command>nasal</command>
<script>doors.crew.toggle();</script>
</binding>
</action>
</animation>

<animation>
<type>translate</type>
<object-name>IntLaPorteduCote</object-name>
<property>sim/model/door-positions/crew/position-norm</property>
<interpolation>
<entry><ind> 0.00 </ind><dep>  0.0 </dep></entry>
<entry><ind> 0.03 </ind><dep> -0.2 </dep></entry>
<entry><ind> 1.00 </ind><dep> -0.2 </dep></entry>
</interpolation>
<axis>
<x>  0 </x>
<y> -1 </y>
<z>  0 </z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>IntLaPorteduCote</object-name>
<property>sim/model/door-positions/crew/position-norm</property>
<interpolation>
<entry><ind> 0.00 </ind><dep>   0 </dep></entry>
<entry><ind> 0.03 </ind><dep>   0 </dep></entry>
<entry><ind> 1.00 </ind><dep> 165 </dep></entry>
</interpolation>
<center>
<x-m> -5.15101 </x-m>
<y-m> -1.94248 </y-m>
<z-m> -2.37726 </z-m>
</center>
<axis>
<x> 0 </x>
<y> 0 </y>
<z> 1 </z>
</axis>
</animation>

<animation>
<type>pick</type>
<object-name>intbaie</object-name>
<object-name>intbaieG</object-name>
<object-name>intbaieD</object-name>
<visible>true</visible>
<action>
<button>0</button>
<repeatable>false</repeatable>
<binding>
<command>nasal</command>
<script>doors.baie.toggle();</script>
</binding>
</action>
</animation>

<animation>
<type>rotate</type>
<object-name>intbaie</object-name>
<property>sim/model/door-positions/baie/position-norm</property>
<factor> 30 </factor>
<center>
<x-m> 12.67527 </x-m>
<y-m>  0.00000 </y-m>
<z-m> -1.03524 </z-m>
</center>
<axis>
<x> 0 </x>
<y> 1 </y>
<z> 0 </z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>intbaieG</object-name>
<property>sim/model/door-positions/baie/position-norm</property>
<factor> 95 </factor>
<axis>
<x1-m>  5.65958 </x1-m>
<y1-m> -1.15391 </y1-m>
<z1-m> -3.27792 </z1-m>
<x2-m>  9.11353 </x2-m>
<y2-m> -1.15391 </y2-m>
<z2-m> -2.06237 </z2-m>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>intbaieD</object-name>
<property>sim/model/door-positions/baie/position-norm</property>
<factor> -95 </factor>
<axis>
<x1-m>  5.65958 </x1-m>
<y1-m>  1.15391 </y1-m>
<z1-m> -3.27792 </z1-m>
<x2-m>  9.11353 </x2-m>
<y2-m>  1.15391 </y2-m>
<z2-m> -2.06237 </z2-m>
</axis>
</animation>

</PropertyList>
