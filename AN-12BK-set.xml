<PropertyList include="AN-12BK-yasim-cnf.xml">
  
  <sim>
    
    <description>Antonov An 12 BK (YASim)</description>
    <author>HerbyW (Instrumentation, Animation, FDM), firefly graphic blender design for interior and panel, BARANGER Emmanuel (3D/FDM-original)</author>
    
    <rating>
      <FDM type="int">     3</FDM>
      <systems type="int"> 3 </systems>
      <cockpit type="int"> 3 </cockpit>
      <model type="int">   4 </model>
    </rating>
    
    <weight n="0">
      <name>Crew</name>
      <weight-lb type="double">600</weight-lb>
      <max-lb type="double">1000</max-lb>
      <min-lb type="double">600</min-lb>
    </weight>
    
    <weight n="1">
      <name>Cargo</name>
      <weight-lb type="double">16000.0</weight-lb>
      <max-lb type="double">44090.0</max-lb>
      <min-lb type="double">0.0</min-lb>
    </weight>
    
    <weight n="2">
      <name>Parachuters a 120 lb</name>
      <weight-lb type="double">0.0</weight-lb>
      <max-lb type="double">12000.0</max-lb>
      <min-lb type="double">0.0</min-lb>
    </weight>
    
    <weight n="3">
      <name>Storage a 6000 lb</name>
      <weight-lb type="double">0.0</weight-lb>
      <max-lb type="double">36000.0</max-lb>
      <min-lb type="double">0.0</min-lb>
    </weight>    
    
    <systems>
      <autopilot>
	<name>Autopilot</name>
	<path>Nasal/AN-12BK-autopilot.xml</path>
      </autopilot>
    </systems>  
    
    <import>
      <path>Huds/Sets/controls.xml</path>
      <x-offset>-100</x-offset>
      <y-offset>70</y-offset>
    </import> 
    
    </sim>   
    
    </PropertyList>